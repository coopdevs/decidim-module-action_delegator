<div class="card">
  <div class="card-divider">
    <h2 class="card-title">
      <%= link_to t("title", scope: "decidim.action_delegator.admin.settings.index"), decidim_admin_action_delegator.settings_path %> &gt;
      <%= t ".title" %>
    </h2>
  </div>

  <div class="card-section">
    <%= form_for @form, url: { action: "update" }, html: { class: "form edit_setting" } do |f| %>
      <%= render partial: "form", object: f %>

      <div class="button--double form-general-submit">
        <%= f.submit t(".save") %>
      </div>
    <% end %>
  </div>
</div>
